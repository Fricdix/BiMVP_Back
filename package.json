    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "jose": "^5.7.0",
    "pdfkit": "^0.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.6",
    "@types/pdfkit": "^0.13.9",
    "prisma": "5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
